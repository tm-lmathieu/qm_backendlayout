{
    "name": "qcmedia/qm_backendlayout",
    "description": "This extension adds a new FAL field into backend_layout records and, if installed, gridelements' backend layout records. It allows the BE user to select an HTML template file to render the content of the page using TypoScript's FLUIDTEMPLATE cObject.",
    "version": "1.0.1",
    "type": "typo3-module",
    "keywords": [
        "typo3",
        "gridelements"
    ],
    "homepage": "https://github.com/qcmedia/qm_backendlayout",
    "time": "2014-06-13",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Michael Miousse",
            "email": "michael.miousse@qcmedia.ca",
            "role": "Developer"
        },
        {
            "name": "Guy Couronn√©",
            "email": "guy.couronne@qcmedia.ca",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/qcmedia/qm_backendlayout/tree/master",
        "issues": "https://github.com/qcmedia/qm_backendlayout/issues"
    },
    "require": {
        "typo3/cms-core": "*"
    },
    "autoload": {
        "classmap": [
            "Classes/"
        ]
    },
    "minimum-stability": "RC",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
